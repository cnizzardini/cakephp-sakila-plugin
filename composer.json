{
    "name": "cnizzardini/cakephp-sakila-plugin",
    "description": "A data seeder based on the MySQL Sakila database for CakePHP 4.x",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": ["cakephp","cakephp seed","cakephp dummy data","cakephp sakila"],
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": ">=4.0",
        "cakephp/migrations": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "php-coveralls/php-coveralls": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Sakila\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sakila\\Test\\": "tests/",
            "TestApp\\": "tests/test_app/App/"
        }
    },
    "support": {
        "issues": "https://github.com/cnizzardini/cakephp-sakila-plugin/issues",
        "wiki": "https://github.com/cnizzardini/cakephp-sakila-plugin/wiki",
        "source": "https://github.com/cnizzardini/cakephp-sakila-plugin"
    },
    "authors": [
        {
            "name": "Chris Nizzardini",
            "role": "Developer"
        }
    ]
}
